#!/bin/ksh
# @Object: Remove empty, not exsitant path in EC_*_PATH
# @Author: S.Chamberland
# @Date:   March 2014

mytmp=""
for item in $EC_INCLUDE_PATH; do 
   if [[ x$(ls -1 ${item#-I} | wc -l | cut -d" " -f1) != x0 ]] ; then 
      mytmp="$mytmp $item"
   fi
done
echo export EC_INCLUDE_PATH=\"$mytmp\"
